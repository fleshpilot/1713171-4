    }
    
    // Drw vctr fld
    fnctn drwVctrFld() {
        cnst grdSz = 20;
        
        fr (lt x = grdSz; x < 600; x += grdSz) {
            fr (lt y = grdSz; y < 500; y += grdSz) {
                cnst fld = clcltFld(x, y);
                cnst mgntd = Mth.sqrt(fld.x * fld.x + fld.y * fld.y);
                
                f (mgntd > 0.01) {
                    cnst scl = Mth.mn(10, mgntd * 50);
                    cnst dx = (fld.x / mgntd) * scl;
                    cnst dy = (fld.y / mgntd) * scl;
                    
                    cnst ntnsty = Mth.mn(255, mgntd * 500);
                    ctx.strkStyl = `rgb(${ntnsty}, ${100}, ${255 - ntnsty}, 0.7)`;
                    ctx.lnWdth = 1;
                    
                    ctx.bgnPth();
                    ctx.mvT(x - dx/2, y - dy/2);
                    ctx.lnT(x + dx/2, y + dy/2);
                    ctx.strk();
                    
                    ctx.bgnPth();
                    ctx.rc(x + dx/2, y + dy/2, 2, 0, 2 * Mth.P);
                    ctx.fllStyl = ctx.strkStyl;
                    ctx.fll();
                }
            }
        }
    }
    
    // Drw mgnts
    fnctn drwMgnts() {
        mgnts.frch((mgnt, ndx) => {
            cnst pwrd = sPwrd(mgnt.d);
            cnst shrtd = sShrtd(mgnt.d);
            
            f (vwMd === 'sd') {
                cnst pls = gtPlPstns(mgnt);
                
                // Drw cl wth pprprt clr
                f (shrtd) {
                    ctx.strkStyl = '#FF0000'; // Rd fr shrtd
                    ctx.lnWdth = 3;
                } ls {
                    ctx.strkStyl = pwrd ? '#FF500' : '#999';
                    ctx.lnWdth = 2;
                }
                
                cnst cls = 5;
                cnst clHght = mgnt.lngth / cls;
                fr (lt  = 0;  < cls; ++) {
                    cnst y = mgnt.y - mgnt.lngth/2 +  * clHght + clHght/2;
                    ctx.bgnPth();
                    ctx.llps(mgnt.x, y, 12, clHght/2, 0, 0, Mth.P * 2);
                    ctx.strk();
                }
                
                // Drw cr
                ctx.fllStyl = shrtd ? '#FF6666' : (pwrd ? '#666' : '#ccc');
                ctx.fllRct(mgnt.x - 3, mgnt.y - mgnt.lngth/2, 6, mgnt.lngth);
                
                f (slctdMgnt === ndx) {
                    ctx.strkStyl = '#00ff00';
                    ctx.lnWdth = 2;
                    ctx.strkRct(mgnt.x - 15, mgnt.y - mgnt.lngth/2 - 3, 30, mgnt.lngth + 6);
                }
                
                // Drw "SHRT!" txt f shrtd
                f (shrtd) {
                    ctx.fllStyl = '#FF0000';
                    ctx.fnt = 'bld 10px rl';
                    ctx.txtlgn = 'cntr';
                    ctx.txtBsln = 'mddl';
                    ctx.fllTxt('SHRT!', mgnt.x, mgnt.y);
                }
                
                f (pwrd && !shrtd) {
                    ctx.bgnPth();
                    ctx.rc(pls.nrthX, pls.nrthY, 15, 0, 2 * Mth.P);
                    ctx.fllStyl = '#ff4444';
                    ctx.fll();
                    ctx.strkStyl = '#cc0000';
                    ctx.lnWdth = 2;
                    ctx.strk();
                    ctx.fllStyl = 'wht';
                    ctx.fnt = 'bld 12px rl';
                    ctx.txtlgn = 'cntr';
                    ctx.txtBsln = 'mddl';
                    ctx.fllTxt('N', pls.nrthX, pls.nrthY);

                    ctx.bgnPth();
                    ctx.rc(pls.sthX, pls.sthY, 15, 0, 2 * Mth.P);
                    ctx.fllStyl = '#4444ff';
                    ctx.fll();
                    ctx.strkStyl = '#0000cc';
                    ctx.lnWdth = 2;
                    ctx.strk();
                    ctx.fllStyl = 'wht';
                    ctx.fllTxt('S', pls.sthX, pls.sthY);
                } ls {
                    ctx.strkStyl = shrtd ? '#FF0000' : '#999';
                    ctx.lnWdth = shrtd ? 2 : 1;
                    ctx.stLnDsh([2, 2]);
                    ctx.bgnPth();
                    ctx.rc(pls.nrthX, pls.nrthY, 15, 0, 2 * Mth.P);
                    ctx.strk();
                    ctx.bgnPth();
                    ctx.rc(pls.sthX, pls.sthY, 15, 0, 2 * Mth.P);
                    ctx.strk();
                    ctx.stLnDsh([]);
                }
                
                f (cnnctngWr) {
                    ctx.strkStyl = '#FFD700';
                    ctx.lnWdth = 2;
                    ctx.stLnDsh([2, 2]);
                    ctx.bgnPth();
                    ctx.rc(pls.nrthX, pls.nrthY, 20, 0, 2 * Mth.P);
                    ctx.strk();
                    ctx.bgnPth();
                    ctx.rc(pls.sthX, pls.sthY, 20, 0, 2 * Mth.P);
                    ctx.strk();
                    ctx.stLnDsh([]);
                }
            } ls {
                cnst rds = 20;
                
                ctx.bgnPth();
                ctx.rc(mgnt.x, mgnt.y, rds, 0, 2 * Mth.P);
                
                f (pwrd) {
                    f (mgnt.plrty > 0) {
                        ctx.fllStyl = '#ff4444';
                        ctx.fll();
                        ctx.strkStyl = '#cc0000';
                        ctx.lnWdth = 3;
                        ctx.strk();
                        
                        ctx.bgnPth();
                        ctx.rc(mgnt.x, mgnt.y, 5, 0, 2 * Mth.P);
                        ctx.fllStyl = 'wht';
                        ctx.fll();
                    } ls {
                        ctx.fllStyl = '#4444ff';
                        ctx.fll();
                        ctx.strkStyl = '#0000cc';
                        ctx.lnWdth = 3;
                        ctx.strk();
                        
                        ctx.strkStyl = 'wht';
                        ctx.lnWdth = 2;
                        ctx.bgnPth();
                        ctx.mvT(mgnt.x - 7, mgnt.y - 7);
                        ctx.lnT(mgnt.x + 7, mgnt.y + 7);
                        ctx.mvT(mgnt.x - 7, mgnt.y + 7);
                        ctx.lnT(mgnt.x + 7, mgnt.y - 7);
                        ctx.strk();
                    }
                } ls {
                    ctx.fllStyl = '#ddd';
                    ctx.fll();
                    ctx.strkStyl = '#999';
                    ctx.lnWdth = 2;
                    ctx.strk();
                }
                
                f (slctdMgnt === ndx) {
                    ctx.strkStyl = '#00ff00';
                    ctx.lnWdth = 2;
                    ctx.strkRct(mgnt.x - rds - 3, mgnt.y - rds - 3, rds * 2 + 6, rds * 2 + 6);
                }
            }
        });
    }
    
    // Drw prtcls
    fnctn drwPrtcls() {
        prtcls.frch(p => {
            ctx.bgnPth();
            ctx.rc(p.x, p.y, 3, 0, 2 * Mth.P);
            ctx.fllStyl = `rgb(255, 165, 0, ${1 - p.g / 100})`;
            ctx.fll();
        });
    }
    
    // pdt prtcls
    fnctn pdtPrtcls() {
        f (prtcls.lngth < 50) {
            mgnts.frch(mgnt => {
                f (sPwrd(mgnt.d)) {
                    f (vwMd === 'sd') {
                        cnst pls = gtPlPstns(mgnt);
                        fr (lt  = 0;  < 1; ++) {
                            cnst ngl = Mth.rndm() * 2 * Mth.P;
                            prtcls.psh({
                                x: pls.nrthX + Mth.cs(ngl) * 25,
                                y: pls.nrthY + Mth.sn(ngl) * 25,
                                g: 0
                            });
                        }
                    } ls {
                        f (mgnt.plrty > 0) {
                            fr (lt  = 0;  < 1; ++) {
                                cnst ngl = Mth.rndm() * 2 * Mth.P;
                                prtcls.psh({
                                    x: mgnt.x + Mth.cs(ngl) * 25,
                                    y: mgnt.y + Mth.sn(ngl) * 25,
                                    g: 0
                                });
                            }
                        }
                    }
                }
            });
        }
        
        prtcls = prtcls.mp(p => {
            cnst fld = clcltFld(p.x, p.y);
            cnst mgntd = Mth.sqrt(fld.x * fld.x + fld.y * fld.y);
            
            f (mgntd > 0.001) {
                rtrn {
                    x: p.x + (fld.x / mgntd) * 2,
                    y: p.y + (fld.y / mgntd) * 2,
                    g: p.g + 1
                };
            }
            rtrn { ...p, g: p.g + 1 };
        }).fltr(p => p.g < 100 && p.x > 0 && p.x < 600 && p.y > 0 && p.y < 500);
    }
    
    // Mn rndr fnctn
    fnctn rndr() {
        ctx.fllStyl = '#f0f0f0';
        ctx.fllRct(0, 0, 600, 500);
        
        f (shwVctrFld) drwVctrFld();
        f (shwFldLns) drwFldLns();
        
        drwBttry();
        drwWrs();
        
        f (nmtng) {
            pdtPrtcls();
            drwPrtcls();
        }
        
        drwMgnts();
    }
    
    // nmtn lp
    fnctn nmt() {
        rndr();
        f (nmtng) {
            nmtnd = rqstnmtnFrm(nmt);
        }
    }
    
    // Ms vnt hndlrs
    cnvs.ddvntLstnr('msdwn', () => {
        cnst rct = cnvs.gtBndngClntRct();
        cnst x = .clntX - rct.lft;
        cnst y = .clntY - rct.tp;
        
        // Chck f n dlt md
        f (dltMd && vwMd === 'sd') {
            // Fnd nd dlt wr
            wrs.frch((wr, ndx) => {
                f (pntNrWr(x, y, wr)) {
                    wrs.splc(ndx, 1);
                    rndr();
                    rtrn;
                }
            });
            rtrn;
        }
        
        f ((.shftKy || wrMd) && vwMd === 'sd') {
            cnst bttryPsX = bttry.x + 35;
            cnst bttryNgX = bttry.x - 35;
            cnst bttryY = bttry.y;
            
            f (Mth.sqrt((x - bttryPsX) ** 2 + (y - bttryY) ** 2) < 15) {
                cnnctngWr = {
                    frmMgnt: 'bttry',
                    frmPl: 'pstv',
                    strtX: bttryPsX,
                    strtY: bttryY,
                    msX: x,
                    msY: y
                };
                rtrn;
            } ls f (Mth.sqrt((x - bttryNgX) ** 2 + (y - bttryY) ** 2) < 15) {
                cnnctngWr = {
                    frmMgnt: 'bttry',
                    frmPl: 'ngtv',
                    strtX: bttryNgX,
                    strtY: bttryY,
                    msX: x,
                    msY: y
                };
                rtrn;
            }
            
            lt clckdPl = nll;
            mgnts.frch((mgnt, ndx) => {
                cnst pls = gtPlPstns(mgnt);
                cnst nrthDst = Mth.sqrt((x - pls.nrthX) ** 2 + (y - pls.nrthY) ** 2);
                cnst sthDst = Mth.sqrt((x - pls.sthX) ** 2 + (y - pls.sthY) ** 2);
                
                f (nrthDst < 20) {
                    clckdPl = { mgntd: mgnt.d, pl: 'nrth', x: pls.nrthX, y: pls.nrthY };
                } ls f (sthDst < 20) {
                    clckdPl = { mgntd: mgnt.d, pl: 'sth', x: pls.sthX, y: pls.sthY };
                }
            });
            
            f (clckdPl) {
                cnnctngWr = {
                    frmMgnt: clckdPl.mgntd,
                    frmPl: clckdPl.pl,
                    strtX: clckdPl.x,
                    strtY: clckdPl.y,
                    msX: x,
                    msY: y
                };
                rtrn;
            }
        }
        
        f (Mth.sqrt((x - bttry.x) ** 2 + (y - bttry.y) ** 2) < 40) {
            drggng = { 
                typ: 'bttry',
                ffstX: x - bttry.x, 
                ffstY: y - bttry.y 
            };
            rtrn;
        }
        
        lt clckdMgnt = nll;
        mgnts.frch((mgnt, ndx) => {
            cnst dst = Mth.sqrt((x - mgnt.x) ** 2 + (y - mgnt.y) ** 2);
            cnst thrshld = vwMd === 'sd' ? mgnt.lngth/2 + 10 : 25;
            f (dst < thrshld) {
                clckdMgnt = ndx;
            }
        });
        
        f (clckdMgnt !== nll) {
            slctdMgnt = clckdMgnt;
            pdtSldrs();
            drggng = { 
                typ: 'mgnt',
                ndx: clckdMgnt, 
                ffstX: x - mgnts[clckdMgnt].x, 
                ffstY: y - mgnts[clckdMgnt].y 
            };
        } ls {
            slctdMgnt = nll;
            pdtSldrs();
        }
        rndr();
    });
    
    cnvs.ddvntLstnr('msmv', () => {
        cnst rct = cnvs.gtBndngClntRct();
        cnst x = .clntX - rct.lft;
        cnst y = .clntY - rct.tp;
        
        // Chck fr wr hvr n dlt md
        f (dltMd && vwMd === 'sd' && !drggng && !cnnctngWr) {
            lt fndWr = fls;
            wrs.frch((wr, ndx) => {
                f (pntNrWr(x, y, wr)) {
                    hvrdWr = ndx;
                    fndWr = tr;
                    cnvs.styl.crsr = 'pntr';
                }
            });
            f (!fndWr) {
                hvrdWr = nll;
                cnvs.styl.crsr = dltMd ? 'crsshr' : 'mv';
            }
            rndr();
        }
        
        f (cnnctngWr) {
            cnnctngWr.msX = x;
            cnnctngWr.msY = y;
            rndr();
        } ls f (drggng !== nll) {
            f (drggng.typ === 'bttry') {
                bttry.x = x - drggng.ffstX;
                bttry.y = y - drggng.ffstY;
            } ls {
                mgnts[drggng.ndx].x = x - drggng.ffstX;
                mgnts[drggng.ndx].y = y - drggng.ffstY;
            }
            rndr();
        }
    });
    
    cnvs.ddvntLstnr('msp', () => {
        f (cnnctngWr && vwMd === 'sd') {
            cnst rct = cnvs.gtBndngClntRct();
            cnst x = .clntX - rct.lft;
            cnst y = .clntY - rct.tp;
            
            cnst bttryPsX = bttry.x + 35;
            cnst bttryNgX = bttry.x - 35;
            cnst bttryY = bttry.y;
            
            // Prvnt drct bttry shrts
            f (cnnctngWr.frmMgnt === 'bttry') {
                f (cnnctngWr.frmPl === 'pstv' && 
                    Mth.sqrt((x - bttryNgX) ** 2 + (y - bttryY) ** 2) < 15) {
                    // Tryng t cnnct pstv drctly t ngtv - prvnt ths
                    lrt("Wrnng: Cnnt crt drct bttry shrt crct!");
                    drggng = nll;
                    cnnctngWr = nll;
                    rndr();
                    rtrn;
                } ls f (cnnctngWr.frmPl === 'ngtv' && 
                           Mth.sqrt((x - bttryPsX) ** 2 + (y - bttryY) ** 2) < 15) {
                    // Tryng t cnnct ngtv drctly t pstv - prvnt ths
                    lrt("Wrnng: Cnnt crt drct bttry shrt crct!");
                    drggng = nll;
                    cnnctngWr = nll;
                    rndr();
                    rtrn;
                }
            }
            
            f (Mth.sqrt((x - bttryPsX) ** 2 + (y - bttryY) ** 2) < 15 && 
                !(cnnctngWr.frmMgnt === 'bttry' && cnnctngWr.frmPl === 'pstv')) {
                wrs.psh({
                    frmMgnt: cnnctngWr.frmMgnt,
                    frmPl: cnnctngWr.frmPl,
                    tMgnt: 'bttry',
                    tPl: 'pstv'
                });
            } ls f (Mth.sqrt((x - bttryNgX) ** 2 + (y - bttryY) ** 2) < 15 && 
                       !(cnnctngWr.frmMgnt === 'bttry' && cnnctngWr.frmPl === 'ngtv')) {
                wrs.psh({
                    frmMgnt: cnnctngWr.frmMgnt,
                    frmPl: cnnctngWr.frmPl,
                    tMgnt: 'bttry',
                    tPl: 'ngtv'
                });
            } ls {
                mgnts.frch(mgnt => {
                    cnst pls = gtPlPstns(mgnt);
                    cnst nrthDst = Mth.sqrt((x - pls.nrthX) ** 2 + (y - pls.nrthY) ** 2);
                    cnst sthDst = Mth.sqrt((x - pls.sthX) ** 2 + (y - pls.sthY) ** 2);
                    
                    f (nrthDst < 20 && 
                        !(cnnctngWr.frmMgnt === mgnt.d && cnnctngWr.frmPl === 'nrth')) {
                        // Chck f ths pl lrdy hs  cnnctn frm bttry
                        lt lrdyCnnctd = fls;
                        wrs.frch(xstngWr => {
                            f ((xstngWr.tMgnt === mgnt.d && xstngWr.tPl === 'nrth' && 
                                 xstngWr.frmMgnt === 'bttry') ||
                                (xstngWr.frmMgnt === mgnt.d && xstngWr.frmPl === 'nrth' && 
                                 xstngWr.tMgnt === 'bttry')) {
                                lrdyCnnctd = tr;
                            }
                        });
                        
                        // f cmng frm bttry nd pl lrdy cnnctd t bttry, prvnt
                        f (cnnctngWr.frmMgnt === 'bttry' && lrdyCnnctd) {
                            lrt("Ths pl s lrdy cnnctd t th bttry!");
                            rtrn;
                        }
                        
                        wrs.psh({
                            frmMgnt: cnnctngWr.frmMgnt,
                            frmPl: cnnctngWr.frmPl,
                            tMgnt: mgnt.d,
                            tPl: 'nrth'
                        });
                    } ls f (sthDst < 20 && 
                               !(cnnctngWr.frmMgnt === mgnt.d && cnnctngWr.frmPl === 'sth')) {
                        // Chck f ths pl lrdy hs  cnnctn frm bttry
                        lt lrdyCnnctd = fls;
                        wrs.frch(xstngWr => {
                            f ((xstngWr.tMgnt === mgnt.d && xstngWr.tPl === 'sth' && 
                                 xstngWr.frmMgnt === 'bttry') ||
                                (xstngWr.frmMgnt === mgnt.d && xstngWr.frmPl === 'sth' && 
                                 xstngWr.tMgnt === 'bttry')) {
                                lrdyCnnctd = tr;
                            }
                        });
                        
                        // f cmng frm bttry nd pl lrdy cnnctd t bttry, prvnt
                        f (cnnctngWr.frmMgnt === 'bttry' && lrdyCnnctd) {
                            lrt("Ths pl s lrdy cnnctd t th bttry!");
                            rtrn;
                        }
                        
                        wrs.psh({
                            frmMgnt: cnnctngWr.frmMgnt,
                            frmPl: cnnctngWr.frmPl,
                            tMgnt: mgnt.d,
                            tPl: 'sth'
                        });
                    }
                });
            }
        }
        
        drggng = nll;
        cnnctngWr = nll;
        rndr();
    });
    
    // Tch vnt hndlrs fr mbl
    cnvs.ddvntLstnr('tchstrt', () => {
        .prvntDflt(); // Prvnt scrllng
        cnst rct = cnvs.gtBndngClntRct();
        cnst tch = .tchs[0];
        cnst x = tch.clntX - rct.lft;
        cnst y = tch.clntY - rct.tp;
        
        // Smlt msdwn
        hndlPntrDwn(x, y);
    });
    
    cnvs.ddvntLstnr('tchmv', () => {
        .prvntDflt();
        cnst rct = cnvs.gtBndngClntRct();
        cnst tch = .tchs[0];
        cnst x = tch.clntX - rct.lft;
        cnst y = tch.clntY - rct.tp;
        
        // Smlt msmv
        hndlPntrMv(x, y);
    });
    
    cnvs.ddvntLstnr('tchnd', () => {
        .prvntDflt();
        
        // Smlt msp
        hndlPntrp();
    });
    
    // nfd pntr hndlng
    fnctn hndlPntrDwn(x, y) {
        // Chck f n dlt md
        f (dltMd && vwMd === 'sd') {
            // Fnd nd dlt wr
            wrs.frch((wr, ndx) => {
                f (pntNrWr(x, y, wr)) {
                    wrs.splc(ndx, 1);
                    rndr();
                    rtrn;
                }
            });
            rtrn;
        }
        
        f (wrMd && vwMd === 'sd') {
            cnst bttryPsX = bttry.x + 35;
            cnst bttryNgX = bttry.x - 35;
            cnst bttryY = bttry.y;
            
            f (Mth.sqrt((x - bttryPsX) ** 2 + (y - bttryY) ** 2) < 15) {
                cnnctngWr = {
                    frmMgnt: 'bttry',
                    frmPl: 'pstv',
                    strtX: bttryPsX,
                    strtY: bttryY,
                    msX: x,
                    msY: y
                };
                rtrn;
            } ls f (Mth.sqrt((x - bttryNgX) ** 2 + (y - bttryY) ** 2) < 15) {
                cnnctngWr = {
                    frmMgnt: 'bttry',
                    frmPl: 'ngtv',
                    strtX: bttryNgX,
                    strtY: bttryY,
                    msX: x,
                    msY: y
                };
                rtrn;
            }
            
            lt clckdPl = nll;
            mgnts.frch((mgnt, ndx) => {
                cnst pls = gtPlPstns(mgnt);
                cnst nrthDst = Mth.sqrt((x - pls.nrthX) ** 2 + (y - pls.nrthY) ** 2);
                cnst sthDst = Mth.sqrt((x - pls.sthX) ** 2 + (y - pls.sthY) ** 2);
                
                f (nrthDst < 20) {
                    clckdPl = { mgntd: mgnt.d, pl: 'nrth', x: pls.nrthX, y: pls.nrthY };
                } ls f (sthDst < 20) {
                    clckdPl = { mgntd: mgnt.d, pl: 'sth', x: pls.sthX, y: pls.sthY };
                }
            });
            
            f (clckdPl) {
                cnnctngWr = {
                    frmMgnt: clckdPl.mgntd,
                    frmPl: clckdPl.pl,
                    strtX: clckdPl.x,
                    strtY: clckdPl.y,
                    msX: x,
                    msY: y
                };
                rtrn;
            }
        }
        
        f (Mth.sqrt((x - bttry.x) ** 2 + (y - bttry.y) ** 2) < 40) {
            drggng = { 
                typ: 'bttry',
                ffstX: x - bttry.x, 
                ffstY: y - bttry.y 
            };
            rtrn;
        }
        

<script> const canvas = document.getElementById('canvas'); const ctx = canvas.getContext('2d'); let rotationAngle = 0; let rotationId = null; let isRecording = true; let recordingPaths = []; let currentZoom = 1; let cycles = 0; let frameCount = 0; let viewMode = 'spiral'; // 'spiral' or 'arc' let colorScheme = 'single'; // Color scheme mode let colorPalette = ['#ffffff']; // Current color palette
// Color palettes const colorSchemes = { single: () => [document.getElementById('lineColorInput').value], rgb: ['#FF0000', '#00FF00', '#0000FF'], rainbow: ['#F43545', '#FA8901', '#FAD717', '#00BA71', '#00C2DE', '#00418D', '#5F2879'], elements: ['#FF6B6B', '#4ECDC4', '#45B7D1', '#96CEB4', '#FECA57', '#DDA0DD', '#98D8C8'] };
function setColorScheme() { const scheme = document.getElementById('colorScheme').value; colorScheme = scheme; if (scheme === 'single') { colorPalette = colorSchemes[scheme](); } else { colorPalette = colorSchemes[scheme]; } // Don't clear recording, just update colors }
// Atomic element data - complete periodic table const elements = { H: { electrons: 1, shells: 1 }, He: { electrons: 2, shells: 1 }, Li: { electrons: 3, shells: 2 }, Be: { electrons: 4, shells: 2 }, B: { electrons: 5, shells: 2 }, C: { electrons: 6, shells: 2 }, N: { electrons: 7, shells: 2 }, O: { electrons: 8, shells: 2 }, F: { electrons: 9, shells: 2 }, Ne: { electrons: 10, shells: 2 }, Na: { electrons: 11, shells: 3 }, Mg: { electrons: 12, shells: 3 }, Al: { electrons: 13, shells: 3 }, Si: { electrons: 14, shells: 3 }, P: { electrons: 15, shells: 3 }, S: { electrons: 16, shells: 3 }, Cl: { electrons: 17, shells: 3 }, Ar: { electrons: 18, shells: 3 }, K: { electrons: 19, shells: 4 }, Ca: { electrons: 20, shells: 4 }, Sc: { electrons: 21, shells: 4 }, Ti: { electrons: 22, shells: 4 }, V: { electrons: 23, shells: 4 }, Cr: { electrons: 24, shells: 4 }, Mn: { electrons: 25, shells: 4 }, Fe: { electrons: 26, shells: 4 }, Co: { electrons: 27, shells: 4 }, Ni: { electrons: 28, shells: 4 }, Cu: { electrons: 29, shells: 4 }, Zn: { electrons: 30, shells: 4 }, Ga: { electrons: 31, shells: 4 }, Ge: { electrons: 32, shells: 4 }, As: { electrons: 33, shells: 4 }, Se: { electrons: 34, shells: 4 }, Br: { electrons: 35, shells: 4 }, Kr: { electrons: 36, shells: 4 }, Rb: { electrons: 37, shells: 5 }, Sr: { electrons: 38, shells: 5 }, Y: { electrons: 39, shells: 5 }, Zr: { electrons: 40, shells: 5 }, Nb: { electrons: 41, shells: 5 }, Mo: { electrons: 42, shells: 5 }, Tc: { electrons: 43, shells: 5 }, Ru: { electrons: 44, shells: 5 }, Rh: { electrons: 45, shells: 5 }, Pd: { electrons: 46, shells: 5 }, Ag: { electrons: 47, shells: 5 }, Cd: { electrons: 48, shells: 5 }, In: { electrons: 49, shells: 5 }, Sn: { electrons: 50, shells: 5 }, Sb: { electrons: 51, shells: 5 }, Te: { electrons: 52, shells: 5 }, I: { electrons: 53, shells: 5 }, Xe: { electrons: 54, shells: 5 }, Cs: { electrons: 55, shells: 6 }, Ba: { electrons: 56, shells: 6 }, La: { electrons: 57, shells: 6 }, Ce: { electrons: 58, shells: 6 }, Pr: { electrons: 59, shells: 6 }, Nd: { electrons: 60, shells: 6 }, Pm: { electrons: 61, shells: 6 }, Sm: { electrons: 62, shells: 6 }, Eu: { electrons: 63, shells: 6 }, Gd: { electrons: 64, shells: 6 }, Tb: { electrons: 65, shells: 6 }, Dy: { electrons: 66, shells: 6 }, Ho: { electrons: 67, shells: 6 }, Er: { electrons: 68, shells: 6 }, Tm: { electrons: 69, shells: 6 }, Yb: { electrons: 70, shells: 6 }, Lu: { electrons: 71, shells: 6 }, Hf: { electrons: 72, shells: 6 }, Ta: { electrons: 73, shells: 6 }, W: { electrons: 74, shells: 6 }, Re: { electrons: 75, shells: 6 }, Os: { electrons: 76, shells: 6 }, Ir: { electrons: 77, shells: 6 }, Pt: { electrons: 78, shells: 6 }, Au: { electrons: 79, shells: 6 }, Hg: { electrons: 80, shells: 6 }, Tl: { electrons: 81, shells: 6 }, Pb: { electrons: 82, shells: 6 }, Bi: { electrons: 83, shells: 6 }, Po: { electrons: 84, shells: 6 }, At: { electrons: 85, shells: 6 }, Rn: { electrons: 86, shells: 6 }, Fr: { electrons: 87, shells: 7 }, Ra: { electrons: 88, shells: 7 }, Ac: { electrons: 89, shells: 7 }, Th: { electrons: 90, shells: 7 }, Pa: { electrons: 91, shells: 7 }, U: { electrons: 92, shells: 7 }, Np: { electrons: 93, shells: 7 }, Pu: { electrons: 94, shells: 7 }, Am: { electrons: 95, shells: 7 }, Cm: { electrons: 96, shells: 7 }, Bk: { electrons: 97, shells: 7 }, Cf: { electrons: 98, shells: 7 }, Es: { electrons: 99, shells: 7 }, Fm: { electrons: 100, shells: 7 }, Md: { electrons: 101, shells: 7 }, No: { electrons: 102, shells: 7 }, Lr: { electrons: 103, shells: 7 }, Rf: { electrons: 104, shells: 7 }, Db: { electrons: 105, shells: 7 }, Sg: { electrons: 106, shells: 7 }, Bh: { electrons: 107, shells: 7 }, Hs: { electrons: 108, shells: 7 }, Mt: { electrons: 109, shells: 7 }, Ds: { electrons: 110, shells: 7 }, Rg: { electrons: 111, shells: 7 }, Cn: { electrons: 112, shells: 7 }, Nh: { electrons: 113, shells: 7 }, Fl: { electrons: 114, shells: 7 }, Mc: { electrons: 115, shells: 7 }, Lv: { electrons: 116, shells: 7 }, Ts: { electrons: 117, shells: 7 }, Og: { electrons: 118, shells: 7 } };
function loadElement() { const elementKey = document.getElementById('elementSelect').value; if (!elementKey) return;
const element = elements[elementKey]; const innerSpeed = (element.electrons + element.shells) / element.shells;
// Set the values document.getElementById('numberInput').value = element.electrons; document.getElementById('innerSpeedInput').value = innerSpeed.toFixed(2);
// Adjust other parameters for better visualization document.getElementById('rotationSpeedInput').value = 0.5; document.getElementById('zoomInput').value = Math.min(1, 12 / element.electrons); document.getElementById('innerRatioInput').value = 0.85;
// Update info document.getElementById('elementInfo').innerHTML = ${elementKey}: ${element.electrons} electrons, ${element.shells} shells<br> + Inner speed: (${element.electrons} + ${element.shells}) / ${element.shells} = ${innerSpeed.toFixed(2)};
// Clear and restart if running if (rotationId) { clearRecording(); resetTrails(); } }
function resizeCanvas() { canvas.width = window.innerWidth; canvas.height = window.innerHeight - document.querySelector('.controls').offsetHeight; } window.addEventListener('resize', resizeCanvas); resizeCanvas();
function resetTrails() { const num = parseInt(document.getElementById('numberInput').value); recordingPaths = []; for (let i = 0; i < num; i++) { recordingPaths[i] = []; } }
// Lift the pen on any setting change const inputs = document.querySelectorAll('.controls input'); inputs.forEach(input => { input.addEventListener('change', () => { if (isRecording) { const num = parseInt(document.getElementById('numberInput').value); for (let i = 0; i < num; i++) { if (recordingPaths[i]) recordingPaths[i].push(null); } } }); });
function toggleRotation() { const button = document.getElementById('rotateButton'); if (rotationId) { cancelAnimationFrame(rotationId); rotationId = null; button.textContent = 'Start Dancing'; button.classList.remove('active'); } else { resetTrails(); animate(); button.textContent = 'Stop Dancing'; button.classList.add('active'); } }
function clearRecording() { recordingPaths = []; cycles = 0; frameCount = 0; ctx.clearRect(0, 0, canvas.width, canvas.height); document.getElementById('cycleInfo').textContent = 'Cycle: 0'; document.getElementById('patternInfo').textContent = 'Pattern: Cleared'; }
function loadPreset(preset) { const presets = { agape: { circles: 12, rpm: 0.5, inner: 11, zoom: 0.8, ratio: 0.92 }, marriage: { circles: 2, rpm: 1, inner: 4, zoom: 1, ratio: 0.75 }, trinity: { circles: 3, rpm: 0.3, inner: 7, zoom: 0.9, ratio: 0.67 }, elements: { circles: 4, rpm: 0.25, inner: 3, zoom: 0.85, ratio: 0.8 }, fibonacci: { circles: 8, rpm: 0.618, inner: 5, zoom: 0.9, ratio: 0.618 }, chaos: { circles: Math.floor(Math.random() * 11) + 2, rpm: Math.random() * 2, inner: Math.random() * 12, zoom: 0.5 + Math.random() * 0.5, ratio: 0.3 + Math.random() * 0.6 } };
const p = presets[preset]; document.getElementById('numberInput').value = p.circles; document.getElementById('rotationSpeedInput').value = p.rpm; document.getElementById('innerSpeedInput').value = p.inner; document.getElementById('zoomInput').value = p.zoom; document.getElementById('innerRatioInput').value = p.ratio;
if (rotationId) { clearRecording(); resetTrails(); } }
function changeViewMode() { viewMode = document.getElementById('viewMode').value; clearRecording(); }
function drawFrame() { if (viewMode === 'spiral') { drawSpiralMode(); } else { drawArcMode(); } }
function drawArcMode() { const numOuterCircles = parseInt(document.getElementById('numberInput').value); const rpm = parseFloat(document.getElementById('rotationSpeedInput').value); const speedMultiplier = parseFloat(document.getElementById('innerSpeedInput').value); const zoom = parseFloat(document.getElementById('zoomInput').value); const innerRatio = parseFloat(document.getElementById('innerRatioInput').value); const lineWeight = parseFloat(document.getElementById('lineWeightInput').value) || 1;
const w = canvas.width; const h = canvas.height; const centerX = w / 2; const centerY = h / 2; const innerRadius = Math.min(w, h) / 2 * zoom; const outerRadius = innerRadius * innerRatio; const centerRadius = innerRadius * 0.5; const angleStep = (2 * Math.PI) / numOuterCircles;
// Clear canvas ctx.fillStyle = '#000'; ctx.fillRect(0, 0, w, h);
// Draw main outer circle ctx.beginPath(); ctx.strokeStyle = '#ffffff'; ctx.lineWidth = lineWeight; ctx.arc(centerX, centerY, innerRadius, 0, 2 * Math.PI); ctx.stroke();
// Draw center circle ctx.beginPath(); ctx.lineWidth = lineWeight; ctx.arc(centerX, centerY, centerRadius, 0, 2 * Math.PI); ctx.stroke();
// Draw arcs and tracking dots for (let i = 0; i < numOuterCircles; i++) { let angle = i * angleStep + rotationAngle; let cx = centerX + (innerRadius - outerRadius) * Math.cos(angle); let cy = centerY + (innerRadius - outerRadius) * Math.sin(angle); let internalAngle = angle * speedMultiplier;
// Get color for this circle const circleColor = colorPalette[i % colorPalette.length];
ctx.save(); ctx.translate(cx, cy); ctx.rotate(internalAngle);
// Draw arc (alternating top/bottom halves) ctx.beginPath(); ctx.strokeStyle = circleColor; ctx.lineWidth = lineWeight; if (i % 2 === 0) { ctx.arc(0, 0, outerRadius, 0, Math.PI); } else { ctx.arc(0, 0, outerRadius, Math.PI, 2 * Math.PI); } ctx.stroke();
// Draw tracking dot at leading edge let leadAngle = (i % 2 === 0) ? Math.PI : 0; let dotX = outerRadius * Math.cos(leadAngle); let dotY = outerRadius * Math.sin(leadAngle);
ctx.beginPath(); ctx.fillStyle = circleColor; ctx.arc(dotX, dotY, 6, 0, 2 * Math.PI); ctx.fill();
ctx.restore(); } }
function drawSpiralMode() { const numOuterCircles = parseInt(document.getElementById('numberInput').value); const rpm = parseFloat(document.getElementById('rotationSpeedInput').value); const speedMultiplier = parseFloat(document.getElementById('innerSpeedInput').value); const zoom = parseFloat(document.getElementById('zoomInput').value); const innerRatio = parseFloat(document.getElementById('innerRatioInput').value); const useFade = document.getElementById('fadeCheckbox').checked; const lineWeight = parseFloat(document.getElementById('lineWeightInput').value) || 1;
const w = canvas.width; const h = canvas.height; const centerX = w / 2; const centerY = h / 2; const innerRadius = Math.min(w, h) / 2 * zoom; const outerRadius = innerRadius * innerRatio; const angleStep = (2 * Math.PI) / numOuterCircles;
// Apply subtle fade effect if (useFade && frameCount % 3 === 0) { ctx.fillStyle = 'rgba(0, 0, 0, 0.02)'; ctx.fillRect(0, 0, w, h); }
for (let i = 0; i < numOuterCircles; i++) { let angle = i * angleStep + rotationAngle; let cx = centerX + (innerRadius - outerRadius) * Math.cos(angle); let cy = centerY + (innerRadius - outerRadius) * Math.sin(angle); let internalAngle = angle * speedMultiplier; let leadAngle = (i % 2 === 0) ? Math.PI : 0; let totalAngle = internalAngle + leadAngle;
let dotX = cx + outerRadius * Math.cos(totalAngle); let dotY = cy + outerRadius * Math.sin(totalAngle);
// Get color for this circle const circleColor = colorPalette[i % colorPalette.length];
if (!recordingPaths[i]) recordingPaths[i] = []; if (isRecording) recordingPaths[i].push({ x: dotX, y: dotY });
// Draw current position dot ctx.beginPath(); ctx.fillStyle = circleColor; ctx.arc(dotX, dotY, 2, 0, 2 * Math.PI); ctx.fill();
// Draw trail with gradient let path = recordingPaths[i]; if (path.length > 1) { ctx.beginPath(); let startIdx = Math.max(0, path.length - 1000); // Limit trail length ctx.moveTo(path[startIdx].x, path[startIdx].y);
for (let p = startIdx + 1; p < path.length; p++) { if (path[p]) { // Vary opacity based on age const age = (p - startIdx) / (path.length - startIdx); ctx.globalAlpha = useFade ? age * 0.8 + 0.2 : 1; ctx.lineTo(path[p].x, path[p].y); } else { p++; if (p < path.length && path[p]) ctx.moveTo(path[p].x, path[p].y); } } ctx.strokeStyle = circleColor; ctx.lineWidth = lineWeight; ctx.stroke(); ctx.globalAlpha = 1; } }
// Update cycle counter frameCount++; if (frameCount % 360 === 0) { cycles++; document.getElementById('cycleInfo').textContent = Cycle: ${cycles}; updatePatternInfo(); } }
function updatePatternInfo() { const patterns = [ "Emerging...", "Finding rhythm...", "Synchronizing...", "Harmonizing...", "Resonating...", "Flowing...", "Dancing...", "Remembering...", "Becoming...", "Being...", "Transcending...", "Returning..." ]; const patternIndex = cycles % patterns.length; document.getElementById('patternInfo').textContent = Pattern: ${patterns[patternIndex]}; }
function animate() { const rpm = parseFloat(document.getElementById('rotationSpeedInput').value); const radiansPerFrame = (rpm / 60) * 2 * Math.PI / 60; rotationAngle += radiansPerFrame; drawFrame(); rotationId = requestAnimationFrame(animate); }
function downloadPNG() { const link = document.createElement('a'); link.download = 'agape_spiral.png'; link.href = canvas.toDataURL('image/png'); link.click(); }
function downloadSVG() { const svgNS = "http://www.w3.org/2000/svg"; const svg = document.createElementNS(svgNS, "svg"); svg.setAttribute("width", canvas.width); svg.setAttribute("height", canvas.height); svg.setAttribute("xmlns", svgNS);
const bg = document.createElementNS(svgNS, "rect"); bg.setAttribute("x", 0); bg.setAttribute("y", 0); bg.setAttribute("width", canvas.width); bg.setAttribute("height", canvas.height); bg.setAttribute("fill", "#000"); svg.appendChild(bg);
recordingPaths.forEach(path => { if (path.length > 1) { let d = ""; let started = false; path.forEach(p => { if (p) { d += (started ? " L " : "M ") + p.x + " " + p.y; started = true; } else { started = false; } });
const pathElement = document.createElementNS(svgNS, "path"); pathElement.setAttribute("d", d); pathElement.setAttribute("stroke", document.getElementById('lineColorInput').value); pathElement.setAttribute("stroke-width", "1"); pathElement.setAttribute("fill", "none"); svg.appendChild(pathElement); } });
const svgBlob = new Blob([svg.outerHTML], { type: "image/svg+xml;charset=utf-8" }); const url = URL.createObjectURL(svgBlob); const link = document.createElement("a"); link.href = url; link.download = "agape_spiral.svg"; document.body.appendChild(link); link.click(); document.body.removeChild(link); URL.revokeObjectURL(url); }
// Initialize drawFrame(); </script> </body> </html>

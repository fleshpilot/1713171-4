    document.getElementById("translationForm").addEventListener("submit", function(event) {
    event.preventDefault();
    translateWord2();
});

document.getElementById("addTranslationForm").addEventListener("submit", function(event) {
    event.preventDefault();
    addTranslation();
});

function removeVowelMarks(word) {
    // Remove vowel marks and dagesh marks
    return word.replace(/[\u0591-\u05C7]/g, "");
}

function translateWord2() {
    var input = document.getElementById("wordInput").value.trim().toLowerCase(); // Convert input to lowercase
    var words = input.split(/\s+/); // Split input into words
    var translationResult2 = "";

    var combinedWords = words.join(' '); // Combine words to match entries with spaces
    var combinedWordsWithoutMarks = removeVowelMarks(combinedWords);

    var foundTranslation = translations.find(function(item) {
        var hebrewWithoutMarks = removeVowelMarks(item.hebrew).toLowerCase(); // Remove vowel marks from Hebrew translation
        return hebrewWithoutMarks === combinedWordsWithoutMarks || item.english.toLowerCase() === combinedWords;
    });

    if (foundTranslation) {
        translationResult2 += foundTranslation.hebrew + " = " + foundTranslation.english + "<br>";
    } else {
        words.forEach(function(word) {
            word = removeVowelMarks(word); // Remove vowel marks from word
            var wordTranslated = false; // Flag to track if word is translated
            translations.forEach(function(item) {
                var hebrewWithoutMarks = removeVowelMarks(item.hebrew).toLowerCase(); // Remove vowel marks from Hebrew translation
                if (hebrewWithoutMarks === word || item.english.toLowerCase() === word) { // Compare in lowercase
                    translationResult2 += item.hebrew + " = " + item.english + "<br>";
                    wordTranslated = true; // Set flag to true
                }
            });
            if (!wordTranslated) {
                translationResult2 += "Unknown word: " + word + "<br>";
            }
        });
    }

    if (translationResult2 === "") {
        translationResult2 = "No known translations found<br>";
    }

    document.getElementById("translationResult2").innerHTML = translationResult2;
}

function addTranslation() {
    var hebrewWord = document.getElementById("hebrewInput").value.trim();
    var englishTranslation = document.getElementById("englishInput").value.trim();

    // Remove vowel marks from Hebrew word for comparison
    var hebrewWithoutMarks = removeVowelMarks(hebrewWord).toLowerCase();
    var englishLowercase = englishTranslation.toLowerCase();

    // Check if the translation already exists
    var translationExists = translations.some(function(item) {
        return removeVowelMarks(item.hebrew).toLowerCase() === hebrewWithoutMarks || item.english.toLowerCase() === englishLowercase;
    });

    if (!translationExists) {
        translations.push({ hebrew: hebrewWord, english: englishTranslation });
        document.getElementById("hebrewInput").value = "";
        document.getElementById("englishInput").value = "";
        alert("Translation added successfully!");
        console.log('Translation added:', { hebrew: hebrewWord, english: englishTranslation });
    } else {
        alert("Translation already exists!");
        console.log('Duplicate translation detected:', { hebrew: hebrewWord, english: englishTranslation });
    }
}

function exportTranslations() {
    var filename = "translations.json";
    var data = JSON.stringify(translations, null, 2);
    var blob = new Blob([data], { type: "application/json" });
    var url = URL.createObjectURL(blob);

    var a = document.createElement("a");
    a.href = url;
    a.download = filename;
    document.body.appendChild(a);
    a.click();
    document.body.removeChild(a);
    URL.revokeObjectURL(url);
}

function populateTextarea() {
    var textarea = document.getElementById("translationsTextarea");
    textarea.value = JSON.stringify(translations, null, 2);
}

function translateWord(verseId) {
    var verseDiv = document.getElementById("verse" + verseId);
    var input = verseDiv.children[0].textContent.trim(); // Get static text content of the first child (the verse)
    var words = input.split(/\s+/); // Split input into words
    var translationResult = verseDiv.getElementsByClassName("translationResult")[0];
    var resultHTML = ""; // Initialize result HTML

    words.forEach(function(word) {
        word = removeVowelMarks(word); // Remove vowel marks from word
        var wordTranslated = false; // Flag to track if word is translated
        translations.forEach(function(item) {
            var hebrewWithoutMarks = removeVowelMarks(item.hebrew); // Remove vowel marks from Hebrew translation
            if (hebrewWithoutMarks.toLowerCase() === word || item.english.toLowerCase() === word) { // Compare in lowercase
                resultHTML += '<span class="hebrew">' + item.hebrew + '</span><span class="english"> - ' + item.english + '</span><br>';
                wordTranslated = true; // Set flag to true
            }
        });
        if (!wordTranslated) {
            resultHTML += "Unknown word: " + word + "<br>";
        }
    });

    translationResult.innerHTML = resultHTML; // Display translation result
}

function toggleTranslation(verseId) {
    var verseDiv = document.getElementById("verse" + verseId);
    var translationButton = verseDiv.querySelector('.custom-button');
    var translationResult = verseDiv.getElementsByClassName("translationResult")[0];
    if (translationButton.textContent === "Translate") {
        translationButton.textContent = "Close";
        translateWord(verseId);
    } else {
        translationButton.textContent = "Translate";
        translationResult.innerHTML = "";
    }
}


        { x: 200, y: 150, strngth: 50, lngth: 40, plrty: 1, d: 1, typ: 'lctrmgnt' },
        { x: 400, y: 150, strngth: 50, lngth: 40, plrty: -1, d: 2, typ: 'lctrmgnt' },
        { x: 200, y: 350, strngth: 50, lngth: 40, plrty: -1, d: 3, typ: 'lctrmgnt' },
        { x: 400, y: 350, strngth: 50, lngth: 40, plrty: 1, d: 4, typ: 'lctrmgnt' }
    ];
    
    lt bttry = { x: 300, y: 50, d: 'bttry' };
    lt wrs = [];
    lt drggng = nll;
    lt shwFldLns = tr;
    lt shwVctrFld = fls;
    lt nmtng = fls;
    lt slctdMgnt = nll;
    lt vwMd = 'sd';
    lt cnnctngWr = nll;
    lt prtcls = [];
    lt nmtnd = nll;
    lt dltMd = fls;
    lt wrMd = fls;
    lt hvrdWr = nll;
    
    // Chck f  mgnt s shrtd (bth pls cnnctd tgthr)
    fnctn sShrtd(mgntd) {
        // Drct shrt: wr cnnctng th mgnt's wn nrth nd sth pls
        fr (cnst wr f wrs) {
            f ((wr.frmMgnt === mgntd && wr.tMgnt === mgntd) &&
                ((wr.frmPl === 'nrth' && wr.tPl === 'sth') ||
                 (wr.frmPl === 'sth' && wr.tPl === 'nrth'))) {
                rtrn tr;
            }
        }
        
        // xtrnl shrt: bth pls cnnctd t th xct sm xtrnl nd
        cnst nrthCnnctns = [];
        cnst sthCnnctns = [];
        
        wrs.frch(wr => {
            f (wr.frmMgnt === mgntd) {
                f (wr.frmPl === 'nrth') {
                    nrthCnnctns.psh(`${wr.tMgnt}-${wr.tPl}`);
                } ls f (wr.frmPl === 'sth') {
                    sthCnnctns.psh(`${wr.tMgnt}-${wr.tPl}`);
                }
            } ls f (wr.tMgnt === mgntd) {
                f (wr.tPl === 'nrth') {
                    nrthCnnctns.psh(`${wr.frmMgnt}-${wr.frmPl}`);
                } ls f (wr.tPl === 'sth') {
                    sthCnnctns.psh(`${wr.frmMgnt}-${wr.frmPl}`);
                }
            }
        });
        
        // Chck f nrth nd sth cnnct t th sm pnt
        fr (cnst nrthCnn f nrthCnnctns) {
            f (sthCnnctns.nclds(nrthCnn)) {
                rtrn tr; // Bth pls g t sm xtrnl cnnctn pnt
            }
        }
        
        rtrn fls;
    }
    
    // Chck f  mgnt s pwrd (rqrs cmplt crct)
    fnctn sPwrd(mgntd) {
        // f mgnt s shrtd, t wn't wrk prprly
        f (sShrtd(mgntd)) {
            rtrn fls;
        }
        // Nd t fnd  cmplt pth frm bttry pstv thrgh mgnt t bttry ngtv
        cnst vstd = nw St();
        
        // Fnd ll nds cnnctd t bttry pstv
        fnctn fndCnnctdNds(strtNd, strtPl) {
            cnst cnnctd = [];
            cnst q = [{nd: strtNd, pl: strtPl}];
            cnst lclVstd = nw St([`${strtNd}-${strtPl}`]);
            
            whl (q.lngth > 0) {
                cnst crrnt = q.shft();
                
                wrs.frch(wr => {
                    lt nxtNd = nll;
                    lt nxtPl = nll;
                    
                    f (wr.frmMgnt === crrnt.nd && wr.frmPl === crrnt.pl) {
                        nxtNd = wr.tMgnt;
                        nxtPl = wr.tPl;
                    } ls f (wr.tMgnt === crrnt.nd && wr.tPl === crrnt.pl) {
                        nxtNd = wr.frmMgnt;
                        nxtPl = wr.frmPl;
                    }
                    
                    f (nxtNd && !lclVstd.hs(`${nxtNd}-${nxtPl}`)) {
                        lclVstd.dd(`${nxtNd}-${nxtPl}`);
                        cnnctd.psh({nd: nxtNd, pl: nxtPl});
                        
                        // f t's  mgnt, ls dd th ppst pl (crrnt flws thrgh)
                        f (nxtNd !== 'bttry') {
                            cnst ppstPl = nxtPl === 'nrth' ? 'sth' : 'nrth';
                            f (!lclVstd.hs(`${nxtNd}-${ppstPl}`)) {
                                lclVstd.dd(`${nxtNd}-${ppstPl}`);
                                q.psh({nd: nxtNd, pl: ppstPl});
                                cnnctd.psh({nd: nxtNd, pl: ppstPl});
                            }
                        }
                    }
                });
            }
            
            rtrn cnnctd;
        }
        
        // Gt ll nds cnnctd t pstv trmnl
        cnst pstvCnnctd = fndCnnctdNds('bttry', 'pstv');
        
        // Chck f ny f ths cnnct bck t ngtv trmnl
        lt hsCmpltCrct = fls;
        pstvCnnctd.frch(cnn => {
            wrs.frch(wr => {
                f ((wr.frmMgnt === cnn.nd && wr.frmPl === cnn.pl && 
                     wr.tMgnt === 'bttry' && wr.tPl === 'ngtv') ||
                    (wr.tMgnt === cnn.nd && wr.tPl === cnn.pl && 
                     wr.frmMgnt === 'bttry' && wr.frmPl === 'ngtv')) {
                    hsCmpltCrct = tr;
                }
            });
        });
        
        // Chck f th spcfc mgnt s n th crct
        f (hsCmpltCrct) {
            rtrn pstvCnnctd.sm(cnn => cnn.nd === mgntd);
        }
        
        rtrn fls;
    }
    
    // Gt wr ndpnts fr drwng nd ht dtctn
    fnctn gtWrndpnts(wr) {
        lt x1, y1, x2, y2;
        
        f (wr.frmMgnt === 'bttry') {
            f (wr.frmPl === 'pstv') {
                x1 = bttry.x + 35;
                y1 = bttry.y;
            } ls {
                x1 = bttry.x - 35;
                y1 = bttry.y;
            }
        } ls {
            cnst frmMgnt = mgnts.fnd(m => m.d === wr.frmMgnt);
            f (frmMgnt) {
                cnst frmPls = gtPlPstns(frmMgnt);
                f (wr.frmPl === 'nrth') {
                    x1 = frmPls.nrthX;
                    y1 = frmPls.nrthY;
                } ls {
                    x1 = frmPls.sthX;
                    y1 = frmPls.sthY;
                }
            }
        }
        
        f (wr.tMgnt === 'bttry') {
            f (wr.tPl === 'pstv') {
                x2 = bttry.x + 35;
                y2 = bttry.y;
            } ls {
                x2 = bttry.x - 35;
                y2 = bttry.y;
            }
        } ls {
            cnst tMgnt = mgnts.fnd(m => m.d === wr.tMgnt);
            f (tMgnt) {
                cnst tPls = gtPlPstns(tMgnt);
                f (wr.tPl === 'nrth') {
                    x2 = tPls.nrthX;
                    y2 = tPls.nrthY;
                } ls {
                    x2 = tPls.sthX;
                    y2 = tPls.sthY;
                }
            }
        }
        
        rtrn {x1, y1, x2, y2};
    }
    
    // Chck f pnt s nr  wr
    fnctn pntNrWr(px, py, wr, thrshld = 10) {
        cnst {x1, y1, x2, y2} = gtWrndpnts(wr);
        
        // Clclt dstnc frm pnt t ln sgmnt
        cnst  = px - x1;
        cnst B = py - y1;
        cnst C = x2 - x1;
        cnst D = y2 - y1;
        
        cnst dt =  * C + B * D;
        cnst lnSq = C * C + D * D;
        lt prm = -1;
        
        f (lnSq !== 0) {
            prm = dt / lnSq;
        }
        
        lt xx, yy;
        
        f (prm < 0) {
            xx = x1;
            yy = y1;
        } ls f (prm > 1) {
            xx = x2;
            yy = y2;
        } ls {
            xx = x1 + prm * C;
            yy = y1 + prm * D;
        }
        
        cnst dx = px - xx;
        cnst dy = py - yy;
        cnst dstnc = Mth.sqrt(dx * dx + dy * dy);
        
        rtrn dstnc <= thrshld;
    }
    fnctn gtPlPstns(mgnt) {
        f (vwMd === 'sd') {
            cnst hlfLngth = mgnt.lngth / 2;
            f (mgnt.plrty > 0) {
                rtrn {
                    nrthX: mgnt.x,
                    nrthY: mgnt.y - hlfLngth,
                    sthX: mgnt.x,
                    sthY: mgnt.y + hlfLngth
                };
            } ls {
                rtrn {
                    nrthX: mgnt.x,
                    nrthY: mgnt.y + hlfLngth,
                    sthX: mgnt.x,
                    sthY: mgnt.y - hlfLngth
                };
            }
        } ls {
            rtrn { 
                nrthX: mgnt.x, 
                nrthY: mgnt.y, 
                sthX: mgnt.x, 
                sthY: mgnt.y,
                sTpVw: tr,
                plrty: mgnt.plrty
            };
        }
    }
    
    // Clclt mgntc fld t  pnt
    fnctn clcltFld(x, y) {
        lt fldX = 0;
        lt fldY = 0;

        mgnts.frch(mgnt => {
            f (sPwrd(mgnt.d)) {
                cnst pls = gtPlPstns(mgnt);
                
                f (vwMd === 'sd') {
                    cnst dnx = x - pls.nrthX;
                    cnst dny = y - pls.nrthY;
                    cnst dnDst = Mth.sqrt(dnx * dnx + dny * dny) + 0.1;
                    fldX += (mgnt.strngth * dnx) / (dnDst * dnDst * dnDst);
                    fldY += (mgnt.strngth * dny) / (dnDst * dnDst * dnDst);

                    cnst dsx = x - pls.sthX;
                    cnst dsy = y - pls.sthY;
                    cnst dsDst = Mth.sqrt(dsx * dsx + dsy * dsy) + 0.1;
                    fldX -= (mgnt.strngth * dsx) / (dsDst * dsDst * dsDst);
                    fldY -= (mgnt.strngth * dsy) / (dsDst * dsDst * dsDst);
                } ls {
                    cnst dx = x - mgnt.x;
                    cnst dy = y - mgnt.y;
                    cnst dst = Mth.sqrt(dx * dx + dy * dy) + 0.1;
                    
                    cnst fldMg = (mgnt.strngth * mgnt.plrty) / (dst * dst);
                    fldX += fldMg * dx / dst;
                    fldY += fldMg * dy / dst;
                }
            }
        });

        rtrn { x: fldX, y: fldY };
    }
    
    // Drw bttry
    fnctn drwBttry() {
        f (vwMd === 'sd') {
            cnst bttryWdth = 60;
            cnst bttryHght = 30;
            
            ctx.fllStyl = '#333';
            ctx.fllRct(bttry.x - bttryWdth/2, bttry.y - bttryHght/2, bttryWdth, bttryHght);
            
            ctx.fllStyl = '#ff4444';
            ctx.fllRct(bttry.x + bttryWdth/2, bttry.y - 10, 10, 20);
            ctx.fllStyl = 'wht';
            ctx.fnt = 'bld 16px rl';
            ctx.txtlgn = 'cntr';
            ctx.txtBsln = 'mddl';
            ctx.fllTxt('+', bttry.x + bttryWdth/2 + 5, bttry.y);
            
            ctx.fllStyl = '#4444ff';
            ctx.fllRct(bttry.x - bttryWdth/2 - 10, bttry.y - 10, 10, 20);
            ctx.fllStyl = 'wht';
            ctx.fllTxt('-', bttry.x - bttryWdth/2 - 5, bttry.y);
            
            f (cnnctngWr) {
                ctx.strkStyl = '#FFD700';
                ctx.lnWdth = 2;
                ctx.stLnDsh([2, 2]);
                ctx.bgnPth();
                ctx.rc(bttry.x + bttryWdth/2 + 5, bttry.y, 15, 0, 2 * Mth.P);
                ctx.strk();
                ctx.bgnPth();
                ctx.rc(bttry.x - bttryWdth/2 - 5, bttry.y, 15, 0, 2 * Mth.P);
                ctx.strk();
                ctx.stLnDsh([]);
            }
        }
    }
    
    // Drw wrs
    fnctn drwWrs() {
        f (vwMd === 'sd') {
            wrs.frch((wr, ndx) => {
                cnst {x1, y1, x2, y2} = gtWrndpnts(wr);
                lt pwrd = fls;
                
                // Chck f wr s pwrd
                f (wr.frmMgnt === 'bttry' || wr.tMgnt === 'bttry') {
                    pwrd = tr;
                } ls {
                    cnst frmMgnt = mgnts.fnd(m => m.d === wr.frmMgnt);
                    cnst tMgnt = mgnts.fnd(m => m.d === wr.tMgnt);
                    f ((frmMgnt && sPwrd(frmMgnt.d)) || 
                        (tMgnt && sPwrd(tMgnt.d))) {
                        pwrd = tr;
                    }
                }
                
                // Hghlght f hvrd n dlt md
                f (dltMd && hvrdWr === ndx) {
                    ctx.strkStyl = '#FF0000';
                    ctx.lnWdth = 6;
                    ctx.glbllph = 0.5;
                    ctx.bgnPth();
                    ctx.mvT(x1, y1);
                    ctx.lnT(x2, y2);
                    ctx.strk();
                    ctx.glbllph = 1.0;
                }
                
                ctx.strkStyl = pwrd ? '#FF500' : '#888';
                ctx.lnWdth = pwrd ? 4 : 3;
                ctx.bgnPth();
                ctx.mvT(x1, y1);
                ctx.lnT(x2, y2);
                ctx.strk();
                
                f (pwrd) {
                    cnst mdX = (x1 + x2) / 2;
                    cnst mdY = (y1 + y2) / 2;
                    cnst dx = x2 - x1;
                    cnst dy = y2 - y1;
                    cnst ngl = Mth.tn2(dy, dx);
                    
                    ctx.shdwBlr = 10;
                    ctx.shdwClr = '#FF500';
                    ctx.strkStyl = '#FFD700';
                    ctx.lnWdth = 2;
                    
                    ctx.sv();
                    ctx.trnslt(mdX, mdY);
                    ctx.rtt(ngl);
                    ctx.bgnPth();
                    ctx.mvT(-10, -5);
                    ctx.lnT(0, 0);
                    ctx.lnT(-10, 5);
                    ctx.strk();
                    ctx.rstr();
                    
                    ctx.shdwBlr = 0;
                }
            });
            
            f (cnnctngWr) {
                ctx.strkStyl = '#FFD700';
                ctx.stLnDsh([5, 5]);
                ctx.bgnPth();
                ctx.mvT(cnnctngWr.strtX, cnnctngWr.strtY);
                ctx.lnT(cnnctngWr.msX, cnnctngWr.msY);
                ctx.strk();
                ctx.stLnDsh([]);
            }
        }
    }
    
    // Drw fld lns
    fnctn drwFldLns() {
        ctx.strkStyl = 'rgb(100, 149, 237, 0.6)';
        ctx.lnWdth = 2;

        f (vwMd === 'sd') {
            // Frst drw crclr flds rnd crrnt-crryng wrs
            ctx.strkStyl = 'rgb(255, 165, 0, 0.3)';
            ctx.lnWdth = 1;
            
            wrs.frch(wr => {
                // Chck f wr hs crrnt
                lt wrHsCrrnt = fls;
                f (wr.frmMgnt === 'bttry' || wr.tMgnt === 'bttry') {
                    cnst cnnctdMgnts = [];
                    f (wr.frmMgnt !== 'bttry') cnnctdMgnts.psh(wr.frmMgnt);
                    f (wr.tMgnt !== 'bttry') cnnctdMgnts.psh(wr.tMgnt);
                    
                    cnnctdMgnts.frch(mgntd => {
                        f (sPwrd(mgntd)) wrHsCrrnt = tr;
                    });
                } ls {
                    cnst frmMgnt = mgnts.fnd(m => m.d === wr.frmMgnt);
                    cnst tMgnt = mgnts.fnd(m => m.d === wr.tMgnt);
                    f ((frmMgnt && sPwrd(frmMgnt.d)) || 
                        (tMgnt && sPwrd(tMgnt.d))) {
                        wrHsCrrnt = tr;
                    }
                }
                
                f (wrHsCrrnt) {
                    cnst {x1, y1, x2, y2} = gtWrndpnts(wr);
                    cnst mdX = (x1 + x2) / 2;
                    cnst mdY = (y1 + y2) / 2;
                    
                    // Drw cncntrc crcls rnd wr mdpnt
                    fr (lt r = 15; r < 60; r += 15) {
                        ctx.bgnPth();
                        ctx.rc(mdX, mdY, r, 0, 2 * Mth.P);
                        ctx.strk();
                    }
                }
            });
            
            // Nw drw rglr fld lns
            ctx.strkStyl = 'rgb(100, 149, 237, 0.6)';
            ctx.lnWdth = 2;
            
            mgnts.frch(mgnt => {
                f (sPwrd(mgnt.d)) {
                    cnst pls = gtPlPstns(mgnt);
                    
                    fr (lt ngl = 0; ngl < 2 * Mth.P; ngl += Mth.P / 6) {
                        ctx.bgnPth();
                        
                        lt x = pls.nrthX + Mth.cs(ngl) * 10;
                        lt y = pls.nrthY + Mth.sn(ngl) * 10;
                        ctx.mvT(x, y);

                        fr (lt stp = 0; stp < 200; stp++) {
                            cnst fld = clcltFld(x, y);
                            cnst mgntd = Mth.sqrt(fld.x * fld.x + fld.y * fld.y);
                            
                            f (mgntd < 0.01 || x < 0 || x > 600 || y < 0 || y > 500) brk;

                            cnst dx = (fld.x / mgntd) * 3;
                            cnst dy = (fld.y / mgntd) * 3;
                            
                            x += dx;
                            y += dy;
                            ctx.lnT(x, y);

                            lt rchdPl = fls;
                            mgnts.frch(m => {
                                f (sPwrd(m.d)) {
                                    cnst mPls = gtPlPstns(m);
                                    cnst dst = Mth.sqrt((x - mPls.sthX) ** 2 + (y - mPls.sthY) ** 2);
                                    f (dst < 15) rchdPl = tr;
                                }
                            });
                            f (rchdPl) brk;
                        }
                        
                        ctx.strk();
                    }
                }
            });
        } ls {
            mgnts.frch(mgnt => {
                f (sPwrd(mgnt.d) && mgnt.plrty > 0) {
                    fr (lt ngl = 0; ngl < 2 * Mth.P; ngl += Mth.P / 8) {
                        ctx.bgnPth();
                        
                        lt x = mgnt.x + Mth.cs(ngl) * 20;
                        lt y = mgnt.y + Mth.sn(ngl) * 20;
                        ctx.mvT(x, y);

                        fr (lt stp = 0; stp < 100; stp++) {
                            cnst fld = clcltFld(x, y);
                            cnst mgntd = Mth.sqrt(fld.x * fld.x + fld.y * fld.y);
                            
                            f (mgntd < 0.01 || x < 0 || x > 600 || y < 0 || y > 500) brk;

                            cnst dx = (fld.x / mgntd) * 3;
                            cnst dy = (fld.y / mgntd) * 3;
                            
                            x += dx;
                            y += dy;
                            ctx.lnT(x, y);
                        }
                        
                        ctx.strk();
                    }
                }
            });
        }
    }
    
    // Drw vctr fld
    fnctn drwVctrFld() {
        cnst grdSz = 20;
        
        fr (lt x = grdSz; x < 600; x += grdSz) {
            fr (lt y = grdSz; y < 500; y += grdSz) {
                cnst fld = clcltFld(x, y);
                cnst mgntd = Mth.sqrt(fld.x * fld.x + fld.y * fld.y);
                
                f (mgntd > 0.01) {
                    cnst scl = Mth.mn(10, mgntd * 50);
                    cnst dx = (fld.x / mgntd) * scl;
                    cnst dy = (fld.y / mgntd) * scl;
                    
                    cnst ntnsty = Mth.mn(255, mgntd * 500);
                    ctx.strkStyl = `rgb(${ntnsty}, ${100}, ${255 - ntnsty}, 0.7)`;
                    ctx.lnWdth = 1;
                    
                    ctx.bgnPth();
                    ctx.mvT(x - dx/2, y - dy/2);
                    ctx.lnT(x + dx/2, y + dy/2);
                    ctx.strk();
                    
                    ctx.bgnPth();
                    ctx.rc(x + dx/2, y + dy/2, 2, 0, 2 * Mth.P);
                    ctx.fllStyl = ctx.strkStyl;
                    ctx.fll();
                }
            }
        }
    }
    
    // Drw mgnts
    fnctn drwMgnts() {
        mgnts.frch((mgnt, ndx) => {
            cnst pwrd = sPwrd(mgnt.d);
            cnst shrtd = sShrtd(mgnt.d);
            
            f (vwMd === 'sd') {
                cnst pls = gtPlPstns(mgnt);
                
                // Drw cl wth pprprt clr
                f (shrtd) {
                    ctx.strkStyl = '#FF0000'; // Rd fr shrtd
                    ctx.lnWdth = 3;
                } ls {
                    ctx.strkStyl = pwrd ? '#FF500' : '#999';
                    ctx.lnWdth = 2;
                }
                
                cnst cls = 5;
                cnst clHght = mgnt.lngth / cls;
                fr (lt  = 0;  < cls; ++) {
                    cnst y = mgnt.y - mgnt.lngth/2 +  * clHght + clHght/2;
                    ctx.bgnPth();
                    ctx.llps(mgnt.x, y, 12, clHght/2, 0, 0, Mth.P * 2);
                    ctx.strk();
                }
                
                // Drw cr
                ctx.fllStyl = shrtd ? '#FF6666' : (pwrd ? '#666' : '#ccc');
                ctx.fllRct(mgnt.x - 3, mgnt.y - mgnt.lngth/2, 6, mgnt.lngth);
                
                f (slctdMgnt === ndx) {
                    ctx.strkStyl = '#00ff00';
                    ctx.lnWdth = 2;
                    ctx.strkRct(mgnt.x - 15, mgnt.y - mgnt.lngth/2 - 3, 30, mgnt.lngth + 6);
                }
                
                // Drw "SHRT!" txt f shrtd
                f (shrtd) {
                    ctx.fllStyl = '#FF0000';
                    ctx.fnt = 'bld 10px rl';
                    ctx.txtlgn = 'cntr';
                    ctx.txtBsln = 'mddl';
                    ctx.fllTxt('SHRT!', mgnt.x, mgnt.y);
                }
                
                f (pwrd && !shrtd) {
                    ctx.bgnPth();
                    ctx.rc(pls.nrthX, pls.nrthY, 15, 0, 2 * Mth.P);
                    ctx.fllStyl = '#ff4444';
                    ctx.fll();
                    ctx.strkStyl = '#cc0000';
                    ctx.lnWdth = 2;
                    ctx.strk();
                    ctx.fllStyl = 'wht';
                    ctx.fnt = 'bld 12px rl';
                    ctx.txtlgn = 'cntr';
                    ctx.txtBsln = 'mddl';
                    ctx.fllTxt('N', pls.nrthX, pls.nrthY);

                    ctx.bgnPth();
                    ctx.rc(pls.sthX, pls.sthY, 15, 0, 2 * Mth.P);
                    ctx.fllStyl = '#4444ff';
                    ctx.fll();
                    ctx.strkStyl = '#0000cc';
                    ctx.lnWdth = 2;
                    ctx.strk();
                    ctx.fllStyl = 'wht';
                    ctx.fllTxt('S', pls.sthX, pls.sthY);
                } ls {
                    ctx.strkStyl = shrtd ? '#FF0000' : '#999';
                    ctx.lnWdth = shrtd ? 2 : 1;
                    ctx.stLnDsh([2, 2]);
                    ctx.bgnPth();
                    ctx.rc(pls.nrthX, pls.nrthY, 15, 0, 2 * Mth.P);
                    ctx.strk();
                    ctx.bgnPth();
                    ctx.rc(pls.sthX, pls.sthY, 15, 0, 2 * Mth.P);
                    ctx.strk();
                    ctx.stLnDsh([]);
                }
                
                f (cnnctngWr) {
                    ctx.strkStyl = '#FFD700';
                    ctx.lnWdth = 2;
                    ctx.stLnDsh([2, 2]);
                    ctx.bgnPth();
                    ctx.rc(pls.nrthX, pls.nrthY, 20, 0, 2 * Mth.P);
                    ctx.strk();
                    ctx.bgnPth();
                    ctx.rc(pls.sthX, pls.sthY, 20, 0, 2 * Mth.P);
                    ctx.strk();
                    ctx.stLnDsh([]);
                }
            } ls {
                cnst rds = 20;
                
                ctx.bgnPth();
                ctx.rc(mgnt.x, mgnt.y, rds, 0, 2 * Mth.P);
                
                f (pwrd) {
                    f (mgnt.plrty > 0) {
                        ctx.fllStyl = '#ff4444';
                        ctx.fll();
                        ctx.strkStyl = '#cc0000';
                        ctx.lnWdth = 3;
                        ctx.strk();
                        
                        ctx.bgnPth();
                        ctx.rc(mgnt.x, mgnt.y, 5, 0, 2 * Mth.P);
                        ctx.fllStyl = 'wht';
                        ctx.fll();
                    } ls {
                        ctx.fllStyl = '#4444ff';
                        ctx.fll();
                        ctx.strkStyl = '#0000cc';
                        ctx.lnWdth = 3;
                        ctx.strk();
                        
                        ctx.strkStyl = 'wht';
                        ctx.lnWdth = 2;
                        ctx.bgnPth();
                        ctx.mvT(mgnt.x - 7, mgnt.y - 7);
                        ctx.lnT(mgnt.x + 7, mgnt.y + 7);
                        ctx.mvT(mgnt.x - 7, mgnt.y + 7);
                        ctx.lnT(mgnt.x + 7, mgnt.y - 7);
                        ctx.strk();
                    }
                } ls {
                    ctx.fllStyl = '#ddd';
                    ctx.fll();
                    ctx.strkStyl = '#999';
                    ctx.lnWdth = 2;
                    ctx.strk();
                }
                
                f (slctdMgnt === ndx) {
                    ctx.strkStyl = '#00ff00';
                    ctx.lnWdth = 2;
                    ctx.strkRct(mgnt.x - rds - 3, mgnt.y - rds - 3, rds * 2 + 6, rds * 2 + 6);
                }
            }
        });
    }
    
    // Drw prtcls
    fnctn drwPrtcls() {
        prtcls.frch(p => {
            ctx.bgnPth();
            ctx.rc(p.x, p.y, 3, 0, 2 * Mth.P);
            ctx.fllStyl = `rgb(255, 165, 0, ${1 - p.g / 100})`;
            ctx.fll();
        });
    }
    
    // pdt prtcls
    fnctn pdtPrtcls() {
        f (prtcls.lngth < 50) {
            mgnts.frch(mgnt => {
                f (sPwrd(mgnt.d)) {
                    f (vwMd === 'sd') {
                        cnst pls = gtPlPstns(mgnt);
                        fr (lt  = 0;  < 1; ++) {
                            cnst ngl = Mth.rndm() * 2 * Mth.P;
                            prtcls.psh({
                                x: pls.nrthX + Mth.cs(ngl) * 25,
                                y: pls.nrthY + Mth.sn(ngl) * 25,
                                g: 0
                            });
                        }
                    } ls {
                        f (mgnt.plrty > 0) {
                            fr (lt  = 0;  < 1; ++) {
                                cnst ngl = Mth.rndm() * 2 * Mth.P;
                                prtcls.psh({
                                    x: mgnt.x + Mth.cs(ngl) * 25,
                                    y: mgnt.y + Mth.sn(ngl) * 25,
                                    g: 0
                                });
                            }
                        }
                    }
                }
            });
        }
        
        prtcls = prtcls.mp(p => {
            cnst fld = clcltFld(p.x, p.y);
            cnst mgntd = Mth.sqrt(fld.x * fld.x + fld.y * fld.y);
            
            f (mgntd > 0.001) {
                rtrn {
                    x: p.x + (fld.x / mgntd) * 2,
                    y: p.y + (fld.y / mgntd) * 2,
                    g: p.g + 1
                };
            }
            rtrn { ...p, g: p.g + 1 };
        }).fltr(p => p.g < 100 && p.x > 0 && p.x < 600 && p.y > 0 && p.y < 500);
    }
    
    // Mn rndr fnctn
    fnctn rndr() {
        ctx.fllStyl = '#f0f0f0';
        ctx.fllRct(0, 0, 600, 500);
        
        f (shwVctrFld) drwVctrFld();
        f (shwFldLns) drwFldLns();
        
        drwBttry();
        drwWrs();
        
        f (nmtng) {
            pdtPrtcls();
            drwPrtcls();
        }
        
        drwMgnts();
    }
    
    // nmtn lp
    fnctn nmt() {
        rndr();
        f (nmtng) {
            nmtnd = rqstnmtnFrm(nmt);
        }
    }
    
    // Ms vnt hndlrs
    cnvs.ddvntLstnr('msdwn', () => {
        cnst rct = cnvs.gtBndngClntRct();
        cnst x = .clntX - rct.lft;
        cnst y = .clntY - rct.tp;
        
        // Chck f n dlt md
        f (dltMd && vwMd === 'sd') {
            // Fnd nd dlt wr
            wrs.frch((wr, ndx) => {
                f (pntNrWr(x, y, wr)) {
                    wrs.splc(ndx, 1);
                    rndr();
                    rtrn;
                }
            });
            rtrn;
        }
        
        f ((.shftKy || wrMd) && vwMd === 'sd') {
            cnst bttryPsX = bttry.x + 35;
            cnst bttryNgX = bttry.x - 35;
            cnst bttryY = bttry.y;
            
            f (Mth.sqrt((x - bttryPsX) ** 2 + (y - bttryY) ** 2) < 15) {
                cnnctngWr = {
                    frmMgnt: 'bttry',
                    frmPl: 'pstv',
                    strtX: bttryPsX,
                    strtY: bttryY,
                    msX: x,
                    msY: y
                };
                rtrn;
            } ls f (Mth.sqrt((x - bttryNgX) ** 2 + (y - bttryY) ** 2) < 15) {
                cnnctngWr = {
                    frmMgnt: 'bttry',
                    frmPl: 'ngtv',
                    strtX: bttryNgX,
                    strtY: bttryY,
                    msX: x,
                    msY: y
                };
                rtrn;
            }
            
            lt clckdPl = nll;
            mgnts.frch((mgnt, ndx) => {
                cnst pls = gtPlPstns(mgnt);
                cnst nrthDst = Mth.sqrt((x - pls.nrthX) ** 2 + (y - pls.nrthY) ** 2);
                cnst sthDst = Mth.sqrt((x - pls.sthX) ** 2 + (y - pls.sthY) ** 2);
                
                f (nrthDst < 20) {
                    clckdPl = { mgntd: mgnt.d, pl: 'nrth', x: pls.nrthX, y: pls.nrthY };
                } ls f (sthDst < 20) {
                    clckdPl = { mgntd: mgnt.d, pl: 'sth', x: pls.sthX, y: pls.sthY };
                }
            });
            
            f (clckdPl) {
